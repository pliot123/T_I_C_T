#include <algorithm>
#include <iostream>
#define INF 987654321
using namespace std;

int arr[101][101];
int main() {
	int n;
	cin >> n;
	int edge;
	cin >> edge;
	int from,to,weight;
	for (int i = 1; i <= n; ++i)
		for (int j = 1; j <= n; ++j)
				arr[i][j] = INF;
	
	for (int i = 0; i < edge; ++i) {
		cin >> from >> to >> weight;
		arr[from][to] =min(arr[from][to], weight);
	}

	for (int i = 1; i <= n; ++i) {
		for (int j = 1; j <= n; ++j) {
			for (int k = 1; k <= n; ++k) {
				if (j == k || j==i || k==i)continue;
				arr[j][k] = min(arr[j][k], arr[j][i] + arr[i][k]);
			}
		}
	}
	for (int i = 1; i <= n; ++i) {
		for (int j = 1; j <= n; ++j) {
			if (arr[i][j] == INF || i==j) {
				cout << 0 << ' ';
			}else
			cout << arr[i][j] << ' ';
		}
		cout << endl;
	}
	return 0;
}
플로이드 문제인데 간선의 가중치를 받을때 똑같은 간선이 여러개 주어져서 왜 자꾸 틀린가했다.
요새 문제를 너무 많이 풀어서 문제를 대충읽게된다 . 알고리즘 얼른 잘하고 싶다 .ㅠ
