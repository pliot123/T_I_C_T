#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
int d[10000];


int main() {
    int n;
    cin >> n;
    
    d[1] = 1;
    d[2] = 3;
    for (int i = 3; i < n+1; i++)
    {
        d[i] = d[i - 1] + d[i - 2] * 2;
    }
    cout << d[n];


    return 0;
}
배열을 이용한 바텀업 방식의 문제
